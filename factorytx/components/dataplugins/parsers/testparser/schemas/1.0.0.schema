$schema: http://json-schema.org/schema
$schema-version: 1.0.0
$plugin-type: acquisition
properties:
  version:
    type: string
  filename_patterns:
    type: array
    items:
      type: string
  id:
    type: string
  sslogs_per_file:
    type: integer
    default: 1
  sslog_type:
    type: string
    default: cycle
required:
- version
- id
