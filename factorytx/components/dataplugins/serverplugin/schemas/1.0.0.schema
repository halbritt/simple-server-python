$schema: http://json-schema.org/schema
$schema-version: 1.0.0
$plugin-type: acquisition
properties:
  host:
    type: string
    default: localhost
  port:
    type: integer
    default: 80
  poll_rate:
    type: integer
    default: 30
  max_size:
    type: integer
    default: 3000000
  max_logs:
    type: integer
    default: 1500
  data_store:
    type: string
    default: /var/spool/sightmachine/factorytx/completed/pollingstore
